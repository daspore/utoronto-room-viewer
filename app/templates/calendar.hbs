<div class="filter-notes">
Search results for
<div class="filter-option">Buildings: {{buildings}}</div>
<div class="filter-option">Rooms: {{rooms}}</div>
<div class="filter-option">
  Capacity: {{if minCapacity minCapacity 'Any'}} - {{if maxCapacity maxCapacity 'Any'}}</div>
<div class="filter-option">Date: {{date}}</div>
</div>

<div class="calendar-header">
  <div class="name">Room Name</div>
  <div class="capacity">Capacity</div>
  <div class="hours">
    <ul class="calendar-list">
      {{#each model.hours as |hour|}}
        <li class="hour">{{hour}}</li>
      {{/each}}
    </ul>
  </div>
  <div class="clear"></div>
</div>

{{#each filteredRooms as |room|}}
  {{calendar-row room=room date=date hours=model.hours}}
{{/each}}
